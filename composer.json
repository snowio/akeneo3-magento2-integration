{
    "name": "snowio/akeneo3-magento2-integration",
    "require": {
        "snowio/magento2-data-model": "^1.0",
        "snowio/transform": "^1.0",
        "snowio/akeneo3-data-model": "dev-master",
        "php-di/php-di": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "SnowIO\\Akeneo3Magento2\\": "src"
        }
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/snowio/"}
    ],
    "autoload-dev": {
        "psr-4": {
            "SnowIO\\Akeneo3Magento2\\Test\\": "test/unit"
        }
    },
    "extra": {
        "php-di-config": "config.php"
    }
}
